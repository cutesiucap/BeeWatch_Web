<script>
    $.ajax({
        url: "../../Order/GetAllOrder",
        type: "GET",
        data: {},
        dataType: "json",
        contentType: "application/json",
        success: function (data) {
            $('#_tatcaOrder').empty();
            $.each(data, function (index, item) {
                var pattern = /Date\(([^)]+)\)/;
                var results = pattern.exec(item.Date_Create);
                var dt = new Date(parseFloat(results[1]));
                $('#_tatcaOrder').append('<div class="row" style="margin-bottom: 5px;"><div class="col-1">#' + item.id + '</div><div class="col-3" style="text-align: center;">' + item.Fullname + ' | #' + item.id_Accounts + '</div><div class="col" style="text-align: center;">' + item.Sum + '</div><div class="col" style="text-align: center;">' + dt.getDate() + '/' + (dt.getMonth() + 1) + '/' + dt.getFullYear() + ' ' + dt.getHours() + ':' + dt.getMinutes() + ':' + dt.getSeconds() + '</div><div class="col" style="text-align: center;">' + item.Status + '</div><div class="col" style="text-align: center;">Chuyển | Hủy</div></div>');
            });
        }
    })
</script>

<script>
    function loadAllOrder() {
        $.ajax({
            url: "../../Order/GetAllOrder",
            type: "GET",
            data: {},
            dataType: "json",
            contentType: "application/json",
            success: function (data) {
                $('#_tatcaOrder').empty();
                $.each(data, function (index, item) {
                    var pattern = /Date\(([^)]+)\)/;
                    var results = pattern.exec(item.Date_Create);
                    var dt = new Date(parseFloat(results[1]));
                    $('#_tatcaOrder').append('<div class="row" style="margin-bottom: 5px;"><div class="col-1">#' + item.id + '</div><div class="col-3" style="text-align: center;">' + item.Fullname + ' | #' + item.id_Accounts + '</div><div class="col" style="text-align: center;">' + item.Sum + '</div><div class="col" style="text-align: center;">' + dt.getDate() + '/' + (dt.getMonth() + 1) + '/' + dt.getFullYear() + ' ' + dt.getHours() + ':' + dt.getMinutes() + ':' + dt.getSeconds() + '</div><div class="col" style="text-align: center;">' + item.Status + '</div><div class="col" style="text-align: center;">Chuyển | Hủy</div></div>');
                });
            }
        })
    }
</script>

