    <script>
        function updatecart() {
            $.ajax({
                url: '../../Cart/Show',
                type: 'GET',
                async: false,
                dataType: 'JSON',
                success: function (newcart) {
                    $('._giohang').empty();
                    $.each(newcart, function (index, item) {
                        $('._giohang').append('<div class="row" style="width: 100%; padding: 5px;"><div class= "col-2" style = "margin: auto;"><img src="'
                            + item.Url_Image + '" class="img-size-50 mr-3" style="width: 40px;"></div ><div class="col-8" style="margin: auto;"><p class="text-muted" style="margin-left: 5px; "><a href="../../Watch/Index/'
                            + item.id_Watch + '">'
                            + item.Name + '</a></p ></div><div class="col-2" style="margin: auto;"><p class="text-muted" style="text-align: right;">'
                            + item.Count + '</p ></div ></div > ');
                    });
                    $('._giohang').parentElement.show;
                }
            });
        };
        window.onload = function () {
            $('._giohang').empty();
